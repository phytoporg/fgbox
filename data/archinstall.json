{
    "audio": "pulseaudio",
    "config_version": "2.5.3",
    "debug": false,
    "harddrives": [
        "/dev/sda"
    ],
    "hostname": "arch-fgbox",
    "mirror-region": {
        "Worldwide": {
            "http://mirror.rackspace.com/archlinux/$repo/os/$arch": true,
            "https://geo.mirror.pkgbuild.com/$repo/os/$arch": true,
            "https://mirror.rackspace.com/archlinux/$repo/os/$arch": true
        }
    },
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "iso"
    },
    "no_pkg_lookups": false,
    "offline": false,
    "packages": [
        "vim"
    ],
    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/minimal.py"
    },
    "script": "fgbox_installer",
    "silent": false,
    "version": "2.5.3",
    "!users": [
        {
            "!password": "password",
            "sudo": true,
            "username": "admin"
        }
    ]
}
